{
  "name": "@damilaredev/utils",
  "version": "0.3.0",
  "description": "collections of utils",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "packageManager": "pnpm@7.1.3",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm typecheck && tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "pnpm run build -- --watch src",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && npm publish --access=public"
  },
  "keywords": [
    "utils",
    "array",
    "collections",
    "string"
  ],
  "author": "Damilare Anjorin",
  "license": "MIT",
  "homepage": "https://github.com/damilaredevone/utils#readme",
  "devDependencies": {
    "@damilaredev/tsconfig": "1.5.1",
    "@types/node": "17.0.34",
    "bumpp": "7.1.1",
    "tsup": "5.12.8",
    "typescript": "4.6.4"
  }
}
