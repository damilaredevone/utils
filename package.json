{
  "name": "@damilaredev/utils",
  "version": "0.5.0",
  "description": "collections of utils",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "packageManager": "pnpm@7.27.0",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": "^14.16.0 || ^16.10.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm typecheck && tsup",
    "watch": "pnpm run build -- --watch src",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "format": "prettier --write --cache --ignore-unknown .",
    "format:fix": "prettier --check --cache --ignore-unknown .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "ci": "ni && nr typecheck && nr lint && nr build",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && npm publish --access=public"
  },
  "keywords": [
    "utils",
    "array",
    "collections",
    "string"
  ],
  "author": "Damilare Anjorin",
  "license": "MIT",
  "homepage": "https://github.com/damilaredevone/utils#readme",
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@damilaredev/eslint-config-typescript": "^0.0.17",
    "@damilaredev/tsconfig": "1.5.2",
    "@types/jest": "^29.2.5",
    "@types/node": "18.13.0",
    "bumpp": "8.2.1",
    "eslint": "^8.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "tsup": "6.6.0",
    "typescript": "4.9.5",
    "vite": "^4.0.4",
    "vitest": "^0.28.0"
  }
}
