{
  "name": "@damilaredev/utils",
  "version": "0.1.2",
  "description": "collections of utils",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm typecheck && tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "pnpm run build -- --watch src",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && npm publish --access=public"
  },
  "keywords": [
    "utils",
    "array",
    "collections",
    "string"
  ],
  "author": "Damilare Anjorin",
  "license": "MIT",
  "homepage": "https://github.com/damilaredevone/utils#readme",
  "devDependencies": {
    "@damilaredev/tsconfig": "^1.5.0",
    "@types/node": "^17.0.19",
    "bumpp": "^7.1.1",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
